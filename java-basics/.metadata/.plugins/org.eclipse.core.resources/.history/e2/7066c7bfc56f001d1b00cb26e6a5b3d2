package com.java.functions;

public class BasicFunction {
	
	public static void main(String[] args) {
		
		System.out.println(Addition(23, 17));
		System.out.println(Substraction(23, 17));
		System.out.println(Multiplication(23, 17));
		System.out.println(Dividation(23, 17));
		
		System.out.println(Square(11));
		System.out.println(Cube(5));
		
	
	}

	static Integer Dividation(Integer num1, Integer num2) {
		// TODO Auto-generated method stub
		return num1 / num2;
	}

	static Integer Multiplication(Integer num1, Integer num2) {
		// TODO Auto-generated method stub
		return num1 * num2;
	}
	static Integer Addition(Integer num1, Integer num2) {
		return num1 + num2;
		
	}
	
	static Integer Substraction(Integer num1, Integer num2){
		return num1-num2;
		
	}
	
	static Integer Square(Integer num1){
		return num1*num1;
		
	}
	
	static Integer Cube(Integer num1){
		return num1*num1*num1;
		
	}
}
